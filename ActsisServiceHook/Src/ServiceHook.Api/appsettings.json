{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
    "MinimumLevel": {
      "Default": "Information"
    },

    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "theme": "Serilog.Sinks.SystemConsole.Themes.AnsiConsoleTheme::Code, Serilog.Sinks.Console"
        }
      },
      {
        "Name": "File",
        "Args": {
          "path": "Logs/info-.log",
          "rollingInterval": "Day",
          "restrictedToMinimumLevel": "Information",
          "shared": true
        }
      },
      {
        "Name": "File",
        "Args": {
          "path": "Logs/error-.log",
          "rollingInterval": "Day",
          "restrictedToMinimumLevel": "Error",
          "shared": true
        }
      }
    ],
    "Enrich": [ "FromLogContext" ]
  },
  "AllowedHosts": "*",
  "BrokerMessageConfiguration": {
    "Exchange": "AzureBoards",
    "Queues": {
      "Insert": {
        "RoutingKey": "Insert",
        "QueueName": "WorkItemInsert"
      },
      "Update": {
        "RoutingKey": "Update",
        "QueueName": "WorkItemUpdate"
      }
    },
    "Users": [
      "sistemas@actsis.com",
      "Sistemas <sistemas@actsis.com>"
    ],
    "IterationPath": [
      "CELSIA",
      "DEVOPS",
      "PRUEBA",
      "PRUEBA2"
    ]
  },
  "RabbitMQ": {
    "Host": "192.168.25.210",
    "Port": 5672,
    "Credentials": {
      "Username": "admin",
      "Password": "N9v!rT6zQ4#xP2kD"
    },
    "MaxRetryAttemps": 3
  }
}
